{
  "multipart": [
    {
      "when": { "up": "true",  "west": "false", "east": "false"},
      "apply": { "model": "villagerswinery:block/grape_vine", "x": 270}
    },
    {
      "when": { "OR": [
        {"up": "true",  "west": "true"},
